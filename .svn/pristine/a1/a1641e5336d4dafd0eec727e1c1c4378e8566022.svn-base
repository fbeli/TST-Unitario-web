package br.com.bradseg.siaw.antecipaparcela.vo;

import java.io.Serializable;

public class RetornoErroVo implements Serializable {	

	/**
	 * 
	 */
	private static final long serialVersionUID = -3473500012364771346L;
	
	private Integer erro;
	private String mensagemErro;
	
	public RetornoErroVo() {

	}

	/**
	 * @return erro
	 */
	public Integer getErro() {
		return erro;
	}

	/**
	 * @param erro Integer
	 */
	public void setErro(Integer erro) {
		this.erro = erro;
	}

	/**
	 * @return mensagemErro
	 */
	public String getMensagemErro() {
		return mensagemErro;
	}

	/**
	 * @param mensagemErro String
	 */
	public void setMensagemErro(String mensagemErro) {
		this.mensagemErro = mensagemErro;
	}
	
}
